{
  "name": "Zephyr Mtg Search",
  "description": "For searching mtg sites",
  "manifest_version": 3,
  "version": "0.1",
  "permissions": [
    "contextMenus",
    "scripting",
    "activeTab"
  ],
  "host_permissions":[
    "https://www.mtggoldfish.com/",
    "https://www.cardmarket.com/",
    "https://*/*",
    "http://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}